{
  "name": "sunpower-cm2",
  "version": "0.1.3",
  "description": "SunPower Connect",
  "author": "SunPower Corporation",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "dev:ios": "concurrently --kill-others 'DEV_PLATFORM=ios npm run live-reload-proxy' 'BROWSER=none npm run start'",
    "dev:android": "concurrently --kill-others 'DEV_PLATFORM=android npm run live-reload-proxy' 'BROWSER=none npm run start'",
    "build": "npm run cleanwww && react-scripts build && cp -a ./build/. ../www/",
    "buildsm": "npm run cleanwww && ./inlineSourceMap.sh && cross-env REACT_APP_ENABLE_LOGGER=true react-scripts build && cp -a ./build/. ../www/",
    "build.ci": "eslint src && cross-env CI=true react-scripts test && cross-env NODE_ENV=production react-scripts build",
    "cleanwww": "find ../www -name 'precache*' -delete && rm -rf ../www/static",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "lint": "eslint --ext .jsx,.js src/",
    "validate-env-files": "node ./gitHooks/envFiles/validate",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "live-reload-proxy": "node ./live-reload/proxy.js",
    "live-reload-precommit": "node ./gitHooks/liveReloadValidate",
    "prettify": "prettier 'src/**/**/*.{js,jsx,scss,md,json}'",
    "sentrifySourceMaps": "node sentrifySourceMaps.js"
  },
  "dependencies": {
    "@sentry/integrations": "^6.9.0",
    "@sentry/tracing": "^6.3.6",
    "@sunpower/nav": "^0.1.38",
    "@sunpower/panel-layout-tool": "^2.1.3",
    "@sunpower/sunpowerimage": "^0.1.2",
    "@sunpower/tabs": "^0.1.38",
    "@sunpower/textfield": "^0.1.5",
    "@sunpower/theme-dark": "^0.2.5",
    "@sunpower/tile": "^0.1.37",
    "@szhsin/react-menu": "^1.8.0",
    "bulma": "^0.7.5",
    "clsx": "^1.0.4",
    "cordova-clipboard": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "final-form": "^4.18.2",
    "jwt-decode": "^2.2.0",
    "loaders.css": "^0.1.2",
    "marked": "^1.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "promise.allsettled": "^1.0.2",
    "ramda": "^0.27.0",
    "react": "^16.9.0",
    "react-block-ui": "^1.3.2",
    "react-circular-progressbar": "^2.0.4",
    "react-card-flip": "^1.1.1",
    "react-click-outside": "github:tj/react-click-outside",
    "react-dom": "^16.9.0",
    "react-final-form-hooks": "^2.0.1",
    "react-ga": "^2.6.0",
    "react-places-autocomplete": "^7.2.1",
    "react-qr-reader": "^2.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.8",
    "react-swipeable-bottom-sheet": "^1.1.1",
    "react-switch": "^6.0.0",
    "react-use-gesture": "^6.0.14",
    "recharts": "^1.7.1",
    "redux": "^4.0.2",
    "redux-act": "^1.7.7",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-observable": "^1.2.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "rpc-websockets": "^5.0.14",
    "rxjs": "^6.5.3",
    "scandit-sdk": "^4.6.3",
    "semver": "^7.3.4",
    "sentry-cordova": "^1.0.0",
    "superagent": "^5.1.2",
    "swagger-client": "^3.13.5",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@sentry/cli": "^1.64.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@svgr/webpack": "^5.5.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.3.0",
    "cors-anywhere": "^0.4.3",
    "cross-env": "^5.2.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.1.0",
    "execa": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^8.2.1",
    "lottie-react": "^2.1.0",
    "msw": "^0.27.0",
    "msw-storybook-addon": "^1.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-scripts": "^4.0.3",
    "replace-in-file": "^6.1.0",
    "storybook-react-router": "^1.0.8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 2 iOS major versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run live-reload-precommit && npm run validate-env-files && npm run lint && npm run prettify && cross-env CI=true npm run  test -- --coverage -o"
    }
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "package.json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "resetMocks": true
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">=10.15.3 <11",
    "npm": ">=6.11.3 <7"
  }
}
